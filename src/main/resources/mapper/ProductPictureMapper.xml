        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="seig.qlj.shopping.mapper.ProductPictureMapper">

    <resultMap id="BaseResultMap" type="seig.qlj.shopping.pojo.ProductPicture">
            <id property="id" column="id" jdbcType="INTEGER"/>
